{
  "name": "project-hub",
  "version": "1.0.0",
  "description": "Central hub for projects",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --coverage --forceExit --runInBand",
    "test:watch": "jest --watchAll --verbose --runInBand",
    "test:ci": "jest --verbose --coverage --ci --forceExit --detectOpenHandles --runInBand",
    "start": "tsc-watch --outDir ./dist --onSuccess 'node --inspect=0.0.0.0:9229 ./dist/src/index'",
    "build": "tsc -b .",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "fixtures": "npx fixtures ./src/fixture --config ormconfig.js"
  },
  "keywords": [
    "project",
    "hub",
    "productivity",
    "code",
    "team",
    "overview"
  ],
  "author": "Team-ProjectHub",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/node-fetch": "^2.5.7",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.4.4",
    "tsc-watch": "^4.2.9",
    "typeorm": "^0.2.29",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^13.13.32",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "prettier": "^2.2.0",
    "supertest": "^6.1.3",
    "tslint": "^6.1.3",
    "sqlite3": "^5.0.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.13.7",
    "tslint-plugin-prettier": "^2.3.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "typeorm-fixtures-cli": "^1.8.1"
  }
}
